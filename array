import java.util.Scanner;  
class A{
    public static void main(String agrs[]){
        int n;
       Scanner sc=new Scanner(System.in);
       System.out.println("enter a limit");
       n=sc.nextInt();
       int a[]=new int[10];
       for(int i=0;i<n;i++)
       {
       a[i]=sc.nextInt();
       }
       System.out.println("the array is");
       for (int i=0; i<n; i++)   
       {  
        System.out.println(a[i]);  
       }  
    }
}
